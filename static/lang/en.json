        "storage": {
            "namespace_id": "storage",
            "name": "Storage Manipulator!",
            "cmd_template": "!storage [...OPTIONS]",
            "desc": "Manipulating the storage file <i>(storage/storage.json)</i> with this command.",
            "options": "-chmod [USERNAME]:[GROUP] => Change the user's role.<br>-prefix [VALUE] => Change the prefix.<br>-part [USERNAME] => safely leave the someone's chat room.",
            "cooldown": "5 sec.",
            "notes": "",
            "min_role": "sup",
            "examples": {
                "Change the user's role": [
                    "!storage chmod monkeos:special",
                    "@ilotterytea, user monkeos (678435820) was successfully assigned to the special group!"
                ],
                "Change the global prefix": [
                    "!storage prefix ~$",
                    "@ilotterytea, the command prefix successfully changed! (~$)"
                ],
                "Part the chat": [
                    "!storage part fedotir",
                    "@ilotterytea, the bot has safely parted from fedotir (431155435) chat room!"
                ]
            }
